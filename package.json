{
  "name": "nextjs-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "postinstall": "yarn prisma:generate && yarn graphql:generate-types && yarn graphql:generate-types-fe",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "chakra:generate-theme-typings": "chakra-cli tokens theme",
    "graphql:generate-types-fe": "graphql-codegen --config graphql-codegen-client.yml",
    "graphql:generate-types": "graphql-codegen --config graphql-codegen-server.yml",
    "prisma:generate": "prisma generate",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:migrate:dev": "prisma migrate dev"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{md,json}": "prettier --write"
  },
  "dependencies": {
