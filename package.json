{
  "name": "nextjs-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"yarn graphql:generate:server -- --watch\" \"yarn graphql:generate:client -- --watch\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "postinstall": "yarn prisma:generate && yarn graphql:generate:client && yarn graphql:generate:server",
    "typecheck": "tsc --noEmit",
    "storybook": "storybook dev -h localhost -p 6006 -s ../public",
    "build-storybook": "storybook build",
    "test": "vitest",
    "graphql:generate:client": "graphql-codegen --config graphql-codegen-client.yml",
    "graphql:generate:server": "graphql-codegen --config graphql-codegen-server.yml",
    "prisma:generate": "prisma generate",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:migrate:dev": "prisma migrate dev"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{md,json}": "prettier --write"
  },
