    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.49.2",
    "react-icons": "4.12.0",
    "reflect-metadata": "0.2.1",
    "tslib": "2.6.2",
    "yup": "^1.3.3",
    "zod": "3.22.4"
